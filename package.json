{
  "name": "eslint-plugin-ts-expect",
  "version": "2.0.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/eslint-plugin-ts-expect.git"
  },
  "author": {
    "name": "4Catalyzer"
  },
  "license": "MIT",
  "scripts": {
    "format": "4c format .",
    "lint": "4c lint src test",
    "tdd": "jest --watch",
    "test": "yarn lint && jest",
    "build": "4c build src -d lib",
    "prepublishOnly": "yarn run build",
    "release": "4c release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c format"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.9.1"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.2",
    "@4c/cli": "^2.1.12",
    "@4c/import-sort": "^4.3.6",
    "@4c/jest-preset": "^1.5.4",
    "@4c/tsconfig": "^0.3.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^6.7.1",
    "eslint-config-4catalyzer": "^1.1.5",
    "eslint-config-4catalyzer-jest": "^2.0.10",
    "eslint-config-4catalyzer-typescript": "^3.0.2",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-import-resolver-workspaces": "^1.0.8",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "bugs": {
    "url": "https://github.com/4Catalyzer/eslint-plugin-ts-expect/issues"
  },
  "homepage": "https://github.com/4Catalyzer/eslint-plugin-ts-expect#readme"
}
