{
  "name": "eslint-plugin-ts-expect",
  "version": "2.1.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/eslint-plugin-ts-expect.git"
  },
  "author": {
    "name": "4Catalyzer"
  },
  "license": "MIT",
  "scripts": {
    "format": "4c format .",
    "lint": "4c lint src test",
    "tdd": "jest --watch",
    "test": "yarn lint && jest",
    "build": "4c build src -d lib",
    "prepublishOnly": "yarn run build",
    "release": "4c release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c format"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "typescript": ">=3.7.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.8.1"
  },
  "devDependencies": {
    "@4c/babel-preset": "^9.1.0",
    "@4c/cli": "^3.0.1",
    "@4c/import-sort": "^4.3.6",
    "@4c/jest-preset": "^1.8.1",
    "@4c/tsconfig": "^0.4.1",
    "@babel/core": "^7.25.9",
    "@babel/preset-typescript": "^7.25.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-4catalyzer": "^1.5.0",
    "eslint-config-4catalyzer-jest": "^2.3.0",
    "eslint-config-4catalyzer-typescript": "^3.3.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-webpack": "^0.13.9",
    "eslint-import-resolver-workspaces": "^1.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/4Catalyzer/eslint-plugin-ts-expect/issues"
  },
  "homepage": "https://github.com/4Catalyzer/eslint-plugin-ts-expect#readme"
}
