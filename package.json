{
  "name": "eslint-plugin-ts-expect",
  "version": "1.0.1",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/eslint-plugin-ts-expect.git"
  },
  "author": {
    "name": "4Catalyzer"
  },
  "license": "MIT",
  "scripts": {
    "format": "4c format .",
    "lint": "4c lint .",
    "tdd": "jest --watch",
    "test": "yarn lint && jest",
    "build": "4c build src -d lib",
    "prepublishOnly": "yarn run build",
    "release": "4c release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c format"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^2.15.0"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.3.6",
    "@4c/cli": "^2.1.10",
    "@4c/jest-preset": "^1.5.1",
    "@4c/tsconfig": "^0.3.1",
    "@babel/preset-typescript": "^7.10.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint-config-4catalyzer": "^1.1.2",
    "eslint-config-4catalyzer-jest": "^2.0.7",
    "eslint-config-4catalyzer-typescript": "^2.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.21.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "bugs": {
    "url": "https://github.com/4Catalyzer/eslint-plugin-ts-expect/issues"
  },
  "homepage": "https://github.com/4Catalyzer/eslint-plugin-ts-expect#readme"
}
